mixins
  #playground
    @contentW 1100
    @contentH 500
    @centerX
    @centerY 100
    @anchorX .5
    @anchorY .5
    @inlineItems
    @itemsMarginX 20
    @itemsMarginY 100

  .sticker
    @w 200
    @h 200
    @anchor .5

  .label
    @font Bebas Neue
    @size 100
    @y 250
    @anchor .5
    @color #fff
    @centerX
